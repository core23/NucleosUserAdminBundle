<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
  <services>
    <service id="Nucleos\UserAdminBundle\Twig\AvatarExtension">
      <tag name="twig.extension"/>
      <argument type="service" id="Nucleos\UserAdminBundle\Avatar\AvatarResolver"/>
      <argument>null</argument>
      <argument type="collection"/>
    </service>
    <service id="Nucleos\UserAdminBundle\Avatar\StaticAvatarResolver">
      <argument>%nucleos_user_admin.default_avatar%</argument>
    </service>
    <service id="Nucleos\UserAdminBundle\Avatar\AvatarResolver" alias="Nucleos\UserAdminBundle\Avatar\StaticAvatarResolver"/>
  </services>
</container>
